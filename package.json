{
  "name": "failhorn",
  "displayName": "Fail Horn ðŸ”Š (FAAAAH)",
  "description": "Plays FAAAAH sound whenever you make a coding error",
  "version": "1.0.1",
  "author": "Monir Hossain",
  "publisher": "Monirweb",
  "repository": {
    "type": "git",
    "url": "https://github.com/monir6163/faah-vs-code-extension"
  },
  "license": "MIT",
  "icon": "icon.png",
  "homepage": "https://github.com/monir6163/faah-vs-code-extension",
  "bugs": {
    "url": "https://github.com/monir6163/faah-vs-code-extension/issues"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "keywords": [
    "error",
    "sound",
    "eslint",
    "typescript",
    "fun"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "faaah.toggleMute",
        "title": "FAAAH: Toggle Mute",
        "icon": "$(unmute)"
      }
    ],
    "configuration": {
      "title": "FAAAH Sound Settings",
      "properties": {
        "faaah.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable FAAAah error sound"
        },
        "faaah.cooldownMs": {
          "type": "number",
          "default": 1500,
          "description": "Minimum delay between sounds (milliseconds)"
        },
        "faaah.muted": {
          "type": "boolean",
          "default": false,
          "description": "Mute FAAAH sounds temporarily"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.3"
  }
}
